{
	"name": "uam/aws-bundle",
	"description": "Symfony bundle to provide a wrapper service around AWS S3",
	"keywords": ["aws", "s3", "cloud", "symfony", "bundle"],
	"homepage": "http://gitweb.united-asian.com",
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Olivier Pichon",
			"email": "op@united-asian.com"
		}
	],
	"require": {
		"symfony/framework-bundle": "^2.0||^3.0",
		"aws/aws-sdk-php": "^3.36",
		"guzzlehttp/guzzle": "^6.0"
	},
	"autoload": {
		"psr-4": { "UAM\\Bundle\\AwsBundle\\": "src/" }
	},
  "extra": {
      "branch-alias": {
          "dev-master": "0.5.*-dev"
      }
  }
}
