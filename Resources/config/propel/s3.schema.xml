<?xml version="1.0" encoding="utf-8"?>
<database name="default" namespace="UAM\Bundle\AwsBundle\Propel" defaultIdMethod="native">
	<table name="s3object" phpName="S3Object" idMethod="native">
		<column name="id"          type="integer" primaryKey="true" autoIncrement="true" required="true" />
		<column name="title"       type="varchar" size="100" required="false" />
		<column name="description" type="longvarchar" required="false" />
		<column name="bucket"      type="varchar" size="63" required="true" />
		<column name="path"        type="varchar" size="255" required="true" />
		<column name="credentials" type="varchar" size="100" required="true" />
		<column name="preauth"     type="varchar" size="100" required="false" default="5 minutes" />
		<column name="filename"    type="varchar" size="255" required="true" />
		<column name="size"        type="integer" required="false" />
		<behavior name="timestampable" />
	</table>
</database>